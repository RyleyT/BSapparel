<body>
    <h1> Subscriber data for <%= user.name %> </h1>
    <table>
        <tr>
            <th>Name</th>
            <td><%= user.name %></td>
        </tr>
        <tr>
            <th>Username</th>
            <td><%= user.username %></td>
        </tr>
        <tr>
            <th>Email</th>
            <td><%= user.email %></td>
        </tr>
        <tr>
            <th>Address</th>
            <td><%= user.address %></td>
        </tr>
    </table>
</body>